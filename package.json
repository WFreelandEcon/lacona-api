{
  "name": "lacona-api",
  "version": "0.1.0",
  "description": "Cross-platform API for use with lacona commands",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers jsx:babel-register --watch-extensions jsx",
    "build": "mkdir -p lib; babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonhorst/lacona-api.git"
  },
  "keywords": [
    "lacona",
    "api"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-api/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-api#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "lodash": "^4.6.1",
    "rxjs": "^5.0.0-beta.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-function-bind"
    ]
  }
}
